@model IEnumerable<King.Azure.Imaging.Entities.ImageEntity>

<div class="container">
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">Streaming <small>(/api/image?file=*File*)</small></h1>
        </div>
        @foreach (var entity in Model)
        {
            <div class="col-lg-3 col-md-4 col-xs-6 thumb">
                <img class="img-responsive" src="/api/image?file=@entity.FileName&width=550&height=200&format=jpg" alt="">
            </div>
        }
    </div>
</div>